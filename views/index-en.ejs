<!-- Contact Section -->
<div id="contact" class="container">
    <div class="card bg-dark-custom">
        <div class="row">
            <!-- Contact Form -->
            <div class="card-body form px-5">
                <h3 class="mt-4"><i class="fas fa-envelope pr-2"></i> Contact me:</h3>
                <form role="form" method="post" action="/contact" id="contact-form">
                    <input type="hidden" name="lang" value="en">
                    <div class="row">
                        <!-- Nom -->
                        <div class="col-md-6">
                            <div class="form-group mb-3">
                                <label for="form-contact-name">Your name</label>
                                <input type="text" id="form-contact-name" class="form-control" name="name" value="<%= typeof formData !== 'undefined' ? formData.name : '' %>" required>
                            </div>
                        </div>
                        <!-- Email -->
                        <div class="col-md-6">
                            <div class="form-group mb-3">
                                <label for="form-contact-email">Your mail</label>
                                <input type="email" id="form-contact-email" class="form-control" name="email" value="<%= typeof formData !== 'undefined' ? formData.email : '' %>" required>
                            </div>
                        </div>
                    </div>
                    <!-- Message -->
                    <div class="form-group mb-3">
                        <label for="form-contact-message">Your message</label>
                        <textarea id="form-contact-message" class="form-control" rows="3" name="message" required><%= typeof formData !== 'undefined' ? formData.message : '' %></textarea>
                    </div>
                    <!-- reCAPTCHA and submit button -->
                    <div class="text-center">
                        <div class="g-recaptcha" data-sitekey="<%= process.env.RECAPTCHA_SITE_KEY %>"></div>
                        <button type="submit" class="btn btn-primary btn-lg text-white">
                            <i class="far fa-paper-plane"></i> Send
                        </button>
                    </div>
                    <br>
                </form>
                
                <!-- Success or Error Messages -->
                <% if (message === 'success') { %>
                    <div class="alert alert-success" role="alert">
                        Your message has been sent successfully! I will get back to you soon.
                    </div>
                <% } else if (message === 'error') { %>
                    <div class="alert alert-danger" role="alert">
                        An error occurred while sending the message. Please try again.
                    </div>
                <% } else if (message === 'recaptcha-failure') { %>
                    <div class="alert alert-warning" role="alert">
                        reCAPTCHA verification failed. Please try again.
                    </div>
                <% } %>
                <% if (typeof anchor !== 'undefined' && anchor) { %>
                    <script>
                        window.location.href = "<%= anchor %>";
                    </script>
                <% } %>

                <!-- Script to fade out the message after 5 seconds -->
                <script>
                    setTimeout(function() {
                        const alert = document.querySelector('.alert');
                        if (alert) {
                            alert.style.transition = "opacity 0.5s ease-out";
                            alert.style.opacity = 0;
                            setTimeout(() => alert.remove(), 500);
                        }
                    }, 5000); // 5 seconds
                </script>
            </div>
            
            <!-- Contact Information -->
            <div class="card-body special-color text-center h-100 white-text">
                <h3 class="my-4 pb-2">Contact information</h3>
                <ul class="text-lg-left list-unstyled ml-4">
                    <li>
                        <p><i class="fas fa-map-marker-alt pr-2"></i>94500 Champigny-sur-Marne IDF, France</p>
                    </li>
                    <li>
                        <p><i class="fas fa-phone pr-2"></i>+33 7 55 31 54 00</p>
